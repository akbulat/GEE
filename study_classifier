//filter specific boundari

var altai = altai1.filter(ee.Filter.eq('name',' '))

//Map.addLayer(altai)
Map.centerObject(altai,7)

//2nd step: u havwe to choose and filter sattelite imagery - Landsat or Sentinel.
var image = ee.ImageCollection("COPERNICUS/S2_SR_HARMONIZED")
        .filterBounds(altai) //where
        .filterMetadata('CLOUDY_PIXEL_PERCENTAGE', 'less_than',1) //how
        .filterDate('2023-05-01','2023-08-30') //when
        .median()
        .clip(altai)
Map.addLayer(image,imageVisParam)

// Supervised Machine Learning method
// 1st step: you have to collect training samples for each classes


// 2nd step: u need to merge all training samples
var samples = water.merge(vegetation).merge(settlement).merge(cropland).merge(cropland_abandoned).merge(snow)
print(samples)

// 3rd step: need to extract the band pixel values for all samples
var bands = (['B2','B3','B4','B5','B6','B7','B8','B11','B12'])
var training = image.select(bands).reduceRegions({
  collection:samples,
  reducer:ee.Reducer.mean(), 
  scale: 10})
print(training)


var classifier = ee.Classifier.smileRandomForest(100).train({
  features:training,
  classProperty: 'Class',
  inputProperties: bands})


var classified = image.select(bands).classify(classifier)

Map.addLayer(classified , {min:0, max:5, palette:['0000FF','00FF00', 'FFFFFF','FFFF00','gray','F0FFFF']})